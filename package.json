{
  "name": "galeries-server",
  "version": "0.1.0",
  "description": "server for Galeries, a web application to share pictures with friends.",
  "main": "src/server.ts",
  "author": "Allan Aoudji <allan.aoudji@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "db:migrate:development": "cross-env NODE_ENV=development sequelize db:migrate",
    "db:migrate:production": "cross-env NODE_ENV=production sequelize db:migrate",
    "db:migrate:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "db:migrate": "yarn db:migrate:development && yarn db:migrate:production && yarn db:migrate:test",
    "db:migrate:undo:development": "cross-env NODE_ENV=development sequelize db:migrate:undo",
    "db:migrate:undo:production": "cross-env NODE_ENV=production sequelize db:migrate:undo",
    "db:migrate:undo:test": "cross-env NODE_ENV=test sequelize db:migrate:undo",
    "db:migrate:undo": "yarn db:migrate:undo:development && yarn db:migrate:undo:production && yarn db:migrate:undo:test",
    "dev": "nodemon",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "migration:generate": "sequelize migration:generate --name",
    "start": "cross-env NODE_ENV=development PORT=2000 ts-node src/index.ts",
    "test": "cross-env NODE_ENV=test PORT=5000 yarn jest --verbose --detectOpenHandles"
  },
  "_moduleAliases": {
    "@sequelize": "./sequelize",
    "@src": "./src",
    "@root": "./"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "2.0.0-beta.1",
    "supertest": "^6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.14",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-node-externals": "^2.5.2"
  }
}
